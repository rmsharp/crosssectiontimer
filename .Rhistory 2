activities <- activities[activities$actor_type == actor, ]
caption <- stri_c("List of possible activities for the ",
stri_replace_all_fixed(actor, pattern = "_",
replacement = "\\_"),
" with assigned frequencies to be used in simulations.")
activities[ , c("activity", "freq")] %>%
kbl(digits = 2, col.names = c("Activity", "Frequency"),
caption = caption, row.names = FALSE) %>%
kable_styling(latex_options = "HOLD_position") %>%
kable_material(c("striped", "hover"))
size <- 3
three_samples <- make_activity_observation(
activities, size = size, activities$freq)
dateTime <- paste0("2023-09-16 12:", c("12:07", "23:18", "54:42"), " CDT")
three_samples_df <-
data.frame(ID = paste0("A", sample(1:10, size, replace = TRUE)),
Actor_Type = actor,
Activity = three_samples,
DateTime = dateTime, stringsAsFactors = FALSE)
caption <- "Example of three rows of data from MTS."
three_samples_df %>%
kbl(col.names = c("Actor ID", "Activity", "Date Time Stamp"),
caption = caption, row.names = FALSE) %>%
kable_styling(latex_options = "HOLD_position") %>%
kable_material(c("striped", "hover"))
three_samples_df %>%
kbl(col.names = c("Actor ID", "Actor Type", "Activity", "Date Time Stamp"),
caption = caption, row.names = FALSE) %>%
kable_styling(latex_options = "HOLD_position") %>%
kable_material(c("striped", "hover"))
?kableExtra::column_spec
three_samples_df <-
data.frame(ID = paste0("A", sample(1:10, size, replace = TRUE)),
Actor_Type = actor,
Activity = three_samples,
DateTime = dateTime, stringsAsFactors = FALSE)
caption <- "Example of three rows of data from MTS."
three_samples_df %>%
kbl(col.names = c("Actor ID", "Actor Type", "Activity", "Date Time Stamp"),
caption = caption, row.names = FALSE) %>%
column_spec(width = c("5em", "7em", "20em", "10em")) %>%
kable_styling(latex_options = "HOLD_position") %>%
kable_material(c("striped", "hover"))
three_samples_df %>%
kbl(col.names = c("Actor ID", "Actor Type", "Activity", "Date Time Stamp"),
caption = caption, row.names = FALSE) %>%
column_spec(1, width = "5em") %>%
column_spec(2, width = "7em") %>%
column_spec(3, width = "20em") %>%
column_spec(4, width = "10em") %>%
kable_styling(latex_options = "HOLD_position") %>%
kable_material(c("striped", "hover"))
three_samples_df %>%
kbl(col.names = c("Actor ID", "Actor Type", "Activity", "Date Time Stamp"),
caption = caption, row.names = FALSE) %>%
column_spec(1, width = "3em") %>%
